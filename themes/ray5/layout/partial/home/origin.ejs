<section id="origin">
  <div class="container">
    <h2 class="text-primary"><%- page.origin.title %></h2>
    <% page.origin.items.forEach(function(value){ %>
    <div class="row">
      <div class="col-lg-2">
        <h3 class="text-primary"><%- value.type %></h3>
      </div>
      <div class="col-lg-6">
        <h4><%- value.title %></h4>
        <p><%- value.content %></p>
      </div>
      <div class="col-lg-4 d-flex justify-content-center align-items-center">
        <div class="image-outer">
          <div
            class="image"
            style="background: url('/image/<%- value.image %>')"
          ></div>
        </div>
      </div>
    </div>
    <% }) %>
  </div>
</section>
