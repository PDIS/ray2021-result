<section id="experience">
  <div class="container">
    <h2>
      <%- page.experience.title %>
    </h2>
    <div class="row">
      <% page.experience.items.forEach(function(value){ %>
        <div class="col-md-6 exp-card">
          <div class="exp-card-text">
            <div class="avatar">
              <div class="circle-avatar">
                <div style="background:url('/image/<%- value.image %>')"></div>
              </div>
              <span>
                <%- value.name %>
              </span>
            </div>
            <a target="_blank" href="<%- value.link %>">
              <div class="content">
                <h3>
                  <%- value.title %>
                    </h4>
                    <p>
                      <%- value.content %>
                    </p>
              </div>
            </a>
          </div>
        </div>
        <% }) %>
    </div>
  </div>
</section>