<section id="origin">
  <div class="container">
    <h2><%- page.origin.title %></h2>
    <% page.origin.items.forEach(function(value){ %>
    <div class="row">
      <div class="col-md-2">
        <h3><%- value.type %></h3>
      </div>
      <div class="col-md-6">
        <h4><%- value.title %></h4>
        <p><%- value.content %></p>
      </div>
      <div class="col-md-4">
        <div
          class="image"
          style="background: url('/image/<%- value.image %>')"
        ></div>
      </div>
    </div>
    <% }) %>
  </div>
</section>
